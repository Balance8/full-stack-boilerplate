{
  "name": "full-stack-boilerplate",
  "version": "0.0.2",
  "author": "Michael Slocum",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "generate": "yarn generate:prisma && yarn generate:nexus && yarn codegen",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P nexus.tsconfig.json src/Api/nexusSchema",
    "codegen": "graphql-codegen --config codegen.yml",
    "create-migration": "prisma migrate dev --preview-feature",
    "migrate-database": "prisma db push --preview-feature"
  },
  "dependencies": {
    "@apollo/client": "3.3.14",
    "@apollo/react-ssr": "4.0.0",
    "@paljs/admin": "2.14.0",
    "@paljs/icons": "1.0.5",
    "@paljs/nexus": "2.14.0",
    "@paljs/ui": "1.1.1",
    "@prisma/client": "2.20.1",
    "apollo-server-micro": "2.22.2",
    "bcryptjs": "2.4.3",
    "cookie": "0.4.1",
    "graphql": "15.5.0",
    "graphql-tag": "2.11.0",
    "graphql-tools": "7.0.4",
    "jsonwebtoken": "8.5.1",
    "next": "10.1.3",
    "nexus": "1.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-quill": "^1.3.5",
    "styled-components": "5.2.3",
    "ts-node": "9.1.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/typescript": "1.21.1",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-react-apollo": "2.2.3",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie": "0.4.0",
    "@types/graphql": "14.5.0",
    "@types/jsonwebtoken": "8.5.1",
    "@types/node": "14.14.37",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/styled-components": "5.1.9",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "babel-plugin-graphql-tag": "3.2.0",
    "babel-plugin-styled-components": "1.12.0",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.23.1",
    "husky": "6.0.0",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "prisma": "2.20.1",
    "typescript": "4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --pattern \"**/*.*(ts|tsx|graphql)\" --staged"
    }
  },
  "cacheDirectories": [
    ".next/cache"
  ],
  "repository": "",
  "description": "new NodeJs Prisma GraphQL TypeScript project"
}
